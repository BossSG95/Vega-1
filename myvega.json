{
  "$schema": "https://vega.github.io/schema/vega-lite/v4.json",
  "description": "My interactive chart",
  "width": 860,
  "height":500,
  "data": {"url": "https://raw.githubusercontent.com/Maplub/AirQualityData/master/PM10_2018_daily.csv"},
 "transform": [{
    "filter": {"field": "pm10", "range": [0, 180]}
  }],
  "selection": {
    "pts": {
      "type": "single"
    }
  },
   "mark": "line",
  "encoding": {
         "x": {"timeUnit":"day","field": "time", "type": "temporal"},

    "y": {"aggregate":"mean","field": "pm10", "type": "quantitative"}
  }
}
